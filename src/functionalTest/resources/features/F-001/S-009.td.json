{
  "title": "must receive an error response when EndTime is less than current time",
  "_guid_": "S-009",
  "_extends_": "F-001_Test_Data_Base",
  "specs": [
    "contains a single Role Assignment",
    "contains EndTime is less than current time"
  ],
  "request": {
    "body": {
      "roleRequest": {
        "reference": "${[scenarioContext][customValues][generateUUID]}"
      },
      "requestedRoles": [
        {
          "_extends_": "RequestedRole",
          "beginTime" : "${[scenarioContext][customValues][nextYear]}T00:00:00Z",
          "endTime" : "${[scenarioContext][customValues][yesterday]}T00:00:00Z"
        }
      ]
    }
  },
  "expectedResponse": {
    "_extends_": "BadRequestResponse",
    "body" : {
      "errorDescription" :  "[[ANYTHING_PRESENT]]"
    }
  }
}
