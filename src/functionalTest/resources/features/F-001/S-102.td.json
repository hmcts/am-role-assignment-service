{
  "title": "must successfully create Role Assignments with begin time and end time have null values",
  "_guid_": "S-102",
  "_extends_": "F-001_Test_Data_Base",
  "specs": [
    "contains begin and end time have null values for ORGANISATION role assignment",
    "contains begin and end time have null values for CASE role assignment"
  ],
  "request": {
    "body": {
      "roleRequest": {
        "reference": "${[scenarioContext][customValues][generateUUID]}"
      },
      "requestedRoles": [
        {
          "_extends_": "S-101_Role_Request_without_time",
          "actorId": "${[scenarioContext][customValues][generateUUID]}",
          "beginTime" : null,
          "endTime" : null
        },
        {
          "_extends_": "S-101_Role_Request_without_time",
          "actorId": "${[scenarioContext][customValues][generateUUID]}",
          "roleType": "CASE",
          "grantType": "SPECIFIC",
          "beginTime" : null,
          "endTime" : null,
          "attributes":
          {
            "caseId": "1234567890666666"
          }
        }
      ]
    }
  },

  "expectedResponse": {
    "_extends_": "Common_201_Response",
    "headers": {
      "Content-Length": "[[ANYTHING_PRESENT]]",
      "Content-Type": "application/json"
    },
    "body": {
      "links": [],
      "roleAssignmentResponse": {
        "roleRequest": {
          "_extends_": "ResponseRoleRequest"
        },
        "requestedRoles": [
          {
            "_extends_": "S-101_Response_without_time",
            "actorId": "${}${[scenarioContext][testData][request][body][requestedRoles][0][actorId]}"
          },
          {
            "_extends_": "S-101_Response_without_time",
            "actorId": "${}${[scenarioContext][testData][request][body][requestedRoles][1][actorId]}",
            "roleType": "${}${[scenarioContext][testData][request][body][requestedRoles][1][roleType]}",
            "grantType": "${}${[scenarioContext][testData][request][body][requestedRoles][1][grantType]}",
            "attributes":
            {
              "caseId": "${}${[scenarioContext][testData][request][body][requestedRoles][1][attributes][caseId]}"
            }
          }
        ]
      }
    }
  }
}
