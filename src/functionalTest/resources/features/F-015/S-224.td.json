{
  "title": "must successfully create allocated-magistrate case role",
  "_guid_": "S-224",
  "_extends_": "F-015_Test_Data_Base",
  "specs": [
    "contains ReplaceExisting is false and reference set to caseId",
    "Befta1 - who is the actor for requested role",
    "contains case-manager case role assignment"
  ],
  "users": {
    "invokingUser": {
      "_extends_": "AmBeftaUser6"
    }
  },
  "request": {
    "body": {
      "roleRequest": {
        "assignerId": "${[scenarioContext][testData][users][invokingUser][id]}",
        "reference": "1607103938250138/case-manager"
      },
      "requestedRoles": [
        {
          "_extends_": "RequestedRole",
          "actorId": "${[scenarioContext][testData][users][befta1][id]}",
          "roleName": "hearing-judge",
          "roleCategory": "JUDICIAL",
          "roleType": "CASE",
          "grantType": "SPECIFIC",
          "attributes":
          {
            "caseId" : "1607103938250138",
            "jurisdiction": "PRIVATELAW",
            "caseType": "PRLAPPS"
          }
        }
      ]
    }
  },
  "expectedResponse": {
    "_extends_": "Common_201_Response",
    "headers": {
      "Content-Length": "[[ANYTHING_PRESENT]]",
      "Content-Type": "application/json"
    },
    "body": {
      "roleAssignmentResponse": {
        "roleRequest": {
          "_extends_": "ResponseRoleRequest",
          "reference": "${[scenarioContext][testData][request][body][roleRequest][reference]}",
          "replaceExisting": true
        },
        "requestedRoles": [
          {
            "_extends_": "ResponseRequestedRole",
            "actorId": "${[scenarioContext][testData][users][befta1][id]}",
            "roleName": "hearing-judge",
            "roleCategory": "JUDICIAL",
            "roleType": "CASE",
            "grantType": "SPECIFIC",
            "attributes":
            {
              "caseId" : "1607103938250138",
              "jurisdiction" : "PRIVATELAW",
              "caseType" : "PRLAPPS"
            }
          }
        ]
      }
    }
  }
}
