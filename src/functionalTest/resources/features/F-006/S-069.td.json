{
  "title": "must successfully receive Role Assignments by both Actor Id and Case Id",
  "_guid_": "S-069",
  "_extends_": "F-006_Test_Data_Base",
  "specs": [
    "contains both Actor Id and Case Id which are assigned to Role Assignments"
  ],

  "request": {
    "queryParams": {
      "actorId": "${[scenarioContext][childContexts][F-006_CreationDataForRoleAssignment][testData][request][body][roleRequest][reference]}",
      "caseId": "${[scenarioContext][childContexts][F-006_CreationDataForRoleAssignment][testData][request][body][requestedRoles][0][attributes][caseId]}"
    }
  },
  "expectedResponse": {
    "_extends_": "Common_200_Response",
    "body": {
      "arrayInMap" : [
        {
          "_extends_": "ResponseRoleAssignment",
          "actorId": "${}${[scenarioContext][testData][request][queryParams][actorId]}",
          "attributes" : {
            "caseId" : "${[scenarioContext][testData][request][queryParams][caseId]}"
          }
        }
      ]
    }
  }
  }
}
